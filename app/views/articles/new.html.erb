<div class= "container">
<h1>New Article</h1>
<%= form_for :article, url: articles_path do |f| %>


  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
</p>
    <%= link_to 'Back to Articles', articles_path %>
<% end %>